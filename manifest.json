{
  "manifest_version": 3,
  "name": "QuickWhatsApp",
  "version": "1.0.0",
  "description": "Quickly open selected phone numbers in WhatsApp, Telegram, and other messaging apps",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options.html"
}
